{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.0@dev",
        "codedungeon/phpunit-result-printer": "0.8.2",
        "dingo/api": "dev-master",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "mpociot/laravel-apidoc-generator": "dev-master",
        "predis/predis": "^2.0@dev",
        "santigarcor/laratrust": "5.0.*",
        "tymon/jwt-auth": "1.0.0-rc.2"
    },
    "require-dev": {
        "ext-json": "*",
        "barryvdh/laravel-debugbar": "^3.2@dev",
        "barryvdh/laravel-ide-helper": "^2.6@dev",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^5.7",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {
            "github.com": "e2ac248cda15bbe31aa37c350f430f84fa6577f5"
        }
    },
    "minimum-stability": "dev"
}
